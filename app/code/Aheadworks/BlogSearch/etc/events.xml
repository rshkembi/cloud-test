<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">

    <event name="aheadworks_blog_api_data_postinterface_save_after">
        <observer name="aheadworks_blog_search_post_save_after" instance="Aheadworks\BlogSearch\Observer\Blog\Post\AfterSave" />
    </event>
    <event name="aheadworks_blog_api_data_postinterface_delete_after">
        <observer name="aheadworks_blog_search_post_delete_after" instance="Aheadworks\BlogSearch\Observer\Blog\Post\AfterDelete"/>
    </event>
    <event name="aw_blog_post_mass_update_after">
        <observer name="aw_blog_post_mass_update_after" instance="Aheadworks\BlogSearch\Observer\Blog\Post\AfterMassUpdatePosts"/>
    </event>

</config>
